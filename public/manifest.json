{
  "manifest_version": 3,

  "name": "TBT Bird",
  "description": "Twitter, Doge",
  "version": "1.0",

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/content_script.js"]
      }
  ],

  "background": {
    "service_worker": "js/background.js"
  },

  "host_permissions": [
    "<all_urls>"
  ]
}
